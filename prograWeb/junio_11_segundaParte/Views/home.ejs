<html>
    <head>
        <title>Page Title</title>
        <link href=/assets/style.css type=text/css rel=stylesheet>
    </head> 

    <body>

        <table>
            <tr>
                <th>Username</th>
                <th>Password</th>
                <th>Id</th>
                <th>Eliminar</th>
                <th>Editar</th>
            </tr>
            <% users.forEach(function(o) { %>
            <tr>
                <td><%= o.username %></td>
                <td><%= o.password %></td>
                <td><%= o._id %></td>
                <td>
                    <form action="/eliminar/<%= o._id %>" method="POST">
                        <button type="submit">Eliminar</button>
                    </form>
                </td>
                <td>
                    <form action="/editar/<%= o._id %>" method="POST">
                        <button type="submit">Editar</button>
                    </form>
                </td>
            </tr>
            <% }); %>
        </table>
        <a href="/">Regresar</a>
</body>
</html>